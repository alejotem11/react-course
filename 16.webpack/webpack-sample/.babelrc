{
  "presets": [
    ["env", {
      "targets": { // Which browser versions do we want to support
        // This is what babel will take into account when compiling our next gen JS to current gen
        "browsers": ["> 1%", "last 2 versions",],
      },
    },],
    "react",
  ],
  "plugins": ["syntax-dynamic-import",],
}